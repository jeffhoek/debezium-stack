{
    "name": "opensearch-connector",
    "config": {
        "name":"opensearch-connector",
        "connector.class": "io.aiven.kafka.connect.opensearch.OpensearchSinkConnector",
        "tasks.max":"1",
        "topics": "mongodb.inventory.cities",
        "connection.url": "http://opensearch-node1:9200",
        "type.name": "city",
        "behavior.on.null.values": "delete",
        "key.ignore": "true",
        "transforms": "RemoveId",
        "transforms.RemoveId.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
        "transforms.RemoveId.blacklist": "_id"
    }
}
